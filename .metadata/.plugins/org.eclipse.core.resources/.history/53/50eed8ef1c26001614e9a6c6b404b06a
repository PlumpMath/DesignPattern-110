package model.board;

import model.player.Player;
import view.BoardView;

public class Facade {

private 	Board board;
//	BoardView boardview;
	
	public Facade(){
		board =new Board();
	//	boardView =new BoardView(null);
		
	}
	
	
	public Board copyBoard()
	{
	return board.copyBoard();
	}
	
	public void placeUnits(Player[] players)
	{
		board.placeUnits(Player[] players);
		
	}
	
	
	
	
}
